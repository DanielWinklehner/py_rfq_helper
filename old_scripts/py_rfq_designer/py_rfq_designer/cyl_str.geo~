Geometry.NumSubEdges = 100; // nicer display of curve
Mesh.CharacteristicLengthMax = 0.025;
h = 0.025;
rmax = 0.1;
zmin = -0.01;
zmax = 1.2876270000000003;
len = zmax - zmin;

Point(1) = { 0, 0, zmin, h };

Point(2) = {rmax,0,zmin,h};
Point(3) = {0,rmax,zmin,h};
Point(4) = {-rmax,0,zmin,h};
Point(5) = {0,-rmax,zmin,h};

Circle(1) = {2,1,3};
Circle(2) = {3,1,4};
Circle(3) = {4,1,5};
Circle(4) = {5,1,2};

Line Loop(5) = {1,2,3,4};
Plane Surface(6) = {5};

out[] = Extrude{0, 0, len} { Surface {6}; };

Physical Surface(0) = {6, out[], };
